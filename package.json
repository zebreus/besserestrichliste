{
	"name": "besserestrichliste",
	"version": "0.0.1",
	"scripts": {
		"dev": "prisma generate && vite dev",
		"build": "prisma generate && vite build",
		"preview": "prisma generate && vite preview",
		"serve": "node build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest --run",
		"test:watch": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"migrate": "prisma migrate dev && prisma db seed -- --environment development",
		"migrate:prod": "prisma migrate deploy && prisma db seed -- --environment production",
		"prepare": "husky",
		"lint-staged": "lint-staged"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/kit": "^2.21.3",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@types/eslint": "^9.6.1",
		"@typescript-eslint/eslint-plugin": "^8.33.1",
		"@typescript-eslint/parser": "^8.33.1",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.9.1",
		"husky": "^9.0.11",
		"lint-staged": "^16.1.0",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"svelte": "^5.33.18",
		"svelte-check": "^4.2.1",
		"ts-node": "^10.9.2",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"unocss": "^66.1.4",
		"vite": "^6.3.5",
		"vitest": "^3.2.3",
		"jsdom": "^26.1.0"
	},
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	},
	"dependencies": {
		"@fontsource/atkinson-hyperlegible": "^5.0.20",
		"@prisma/client": "=6.7.0",
		"@unocss/extractor-svelte": "^66.1.4",
		"@unocss/reset": "^66.1.4",
		"prisma": "=6.7.0",
		"tsx": "^4.10.1"
	},
	"overrides": {
		"svelte-hmr": {
			"svelte": "^5.0.0-next.131"
		}
	},
	"type": "module",
	"bin": "./build",
	"license": "MIT",
	"files": [
		"build",
		"prisma",
		"static"
	],
	"lint-staged": {
		"*.+(ts|mts|cts|js|mjs|cjs|svelte)": [
			"eslint --fix"
		],
		"*": [
			"prettier --write -u"
		]
	}
}
